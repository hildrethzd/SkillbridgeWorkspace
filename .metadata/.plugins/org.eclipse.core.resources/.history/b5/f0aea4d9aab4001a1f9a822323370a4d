package com.skillstorm;

public class LoopExample {

	public static void main(String[] args) {		

	}
	
	
	public void dowhile() {
		System.out.println("************ DOWHILE *****************");
		
		int x = 5;
		
		while (x < 5) {
			System.out.println(" While: " + x);
		}
		
		
		do {
			System.out.println(" Do: " + x);
		} while (x < 5);
		
		
		
	}

}
